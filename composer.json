{
    "name": "antodippo/testing-playground",
    "description": "Testing playground with mutation tests",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonello D'Ippolito",
            "email": "antonellodippolito@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "infection/infection": "^0.20",
        "steos/quickcheck": "^2.0",
        "giorgiosironi/eris": "^0.11.0",
        "phpspec/phpspec": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
